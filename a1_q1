#include<iostream>
using namespace std;

int main()
{
    for(int i=1; i<=5; i=i+1)
    cout<<"*";
    
   
    for(int i=1; i<=3; i=i+1)
    {
        for(int j=1;j<=(5-i-1);j=j+1)
        {
            cout<<" ";
        }
        cout<<"*"<<endl;
    }   
    
    for(int i=1; i<=5; i=i+1)
    cout<<"*";
    return 0;
}
