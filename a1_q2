#include<iostream>
using namespace std;
int main()
{
    int marks;
    cout<<"input marks of student=";
    cin>>marks;
    if(marks>90)
    cout<<"excellent";
    else if(marks>80 && marks<=90)
    cout<<"good";
    else if(marks>70 && marks<=80)
    cout<<"fair";
    else if(marks>60 && marks<=70)
    cout<<"meets expection";
    else if(marks<60)
    cout<<"below par";
    return 0;

}
